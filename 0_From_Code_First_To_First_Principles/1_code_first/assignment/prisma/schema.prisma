// prisma/schema.prisma

generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma-client"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://postgres:postgres@localhost:5432/postgres"
}

model User {
  id          Int     @id @default(autoincrement())
  email       String  @unique
  firstName   String
  lastName    String
  userName    String  @unique
  password    String
}
